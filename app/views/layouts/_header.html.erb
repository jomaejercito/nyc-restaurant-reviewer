<header>
<% if !logged_in? %>
  <%= link_to("Log In", login_path) %>
  <%= link_to("Sign Up", signup_path) %>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %>
<% else %>
  <%= link_to("My Reviews", user_reviews_path(current_user)) %>
  <%= link_to("All Restaurants", restaurants_path) %>
  <%= link_to("Restaurants by Cuisine", cuisines_path) %>
  <%= link_to("Restaurants by Neighborhood", neighborhoods_path) %>
  <%= link_to("Reviewers", users_path) %>
  <%= link_to("Log Out", logout_path) %>
<% end %>
</header>
